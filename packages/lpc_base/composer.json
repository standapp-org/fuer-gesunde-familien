{
    "name": "lpc/lpc_base",
    "type": "typo3-cms-extension",
    "description": "LPC base extension",
    "homepage": "https://www.laupercomputing.ch",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS"],
    "version": "2.4.1",
    "prefer-stable": true,
    "require": {
        "typo3/cms-core": "^12.4 || ^11.5 || ^10.4 || ^9.5 || ^8.7",
        "bacon/bacon-qr-code": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LPC\\LpcBase\\": "Classes"
        }
    },
    "replace": {
        "lpc/lpc_base": "self.version",
        "typo3-ter/lpc-base": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "lpc_base"
        }
    }
}
