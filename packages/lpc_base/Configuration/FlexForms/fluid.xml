						</TCEforms>
					</switchableControllerActions>
					<settings.fluid>
						<TCEforms>
							<label>Fluid</label>
							<config>
								<type>text</type>
								<renderType>t3editor</renderType>
								<format>html</format>
								<rows>20</rows>
							</config>
