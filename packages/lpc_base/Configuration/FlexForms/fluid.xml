							<label>Cache output?</label>
							<config>
								<type>radio</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">cached</numIndex>
										<numIndex index="1">Fluid->renderCached</numIndex>
									</numIndex>
									<numIndex index="1">
