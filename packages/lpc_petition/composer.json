{
    "name": "lpc/petition",
    "type": "typo3-cms-extension",
    "description": "Petition",
    "version": "12.0.0",
    "require": {
        "typo3/cms-core": "^12.4",
		"lpc/lpc_base": ">=2.4.1",
        "lpc/captcha": "*",
        "sjbr/static-info-tables": "*"
    },
    "autoload": {
        "psr-4": {
            "LPC\\LpcPetition\\": "Classes"
        }
    },
    "replace": {
        "lpc/petition": "self.version",
        "typo3-ter/lpc-petition": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "lpc_petition"
        }
    }
}
